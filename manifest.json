{
    "manifest_version": 3,
    "name": "Clickfirm",
    "description": "Confirm AWS actions with a click",
    "version": "1.0",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.console.aws.amazon.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}